@model EducareApplication.EducareService.AdmissionTable

@{
    ViewBag.Title = "AdmissionPageGenerater";
}


<div class="card" style=" margin-top:100px;" id="registrationform">
    <div class="row">
        <div class="col-lg-6">
            <img src="@Model.ImgURL.ToString()" class="card-img-top" alt="student_Profile_Image">

            <br />
            <br />
            <strong class="text-justify" style="margin-left:10px;"> <strong>@Model.STU_NAME.ToString() @Model.STU_F_NAME.ToString()</strong></strong>

        </div>
        <div class="col-lg-6 text-right" style="padding:50px;">

            @if (ViewBag.Success != null)
            {
                <div class="alert alert-success">
                    <div class="alert-heading"><strong><span class="fa fa-check"></span> Registration was successful.</strong> <button class="btn btn-sm btn-outline-success text-lg-left" style="margin-left:180px;" onclick="$('.alert').hide()"><span class="fa fa-trash"></span></button></div>
                </div>
            }

            <Strong> Full Name: @Model.STU_NAME.ToString() @Model.STU_F_NAME.ToString()</Strong>
            <br />
            <Strong>Address: @Model.STU_CITY.ToString() @Model.STU_STATE.ToString()  @Model.STU_COUNTRY.ToString()</Strong>
            <br />

            <Strong>Contact Information : @Model.STU_MOBILE.ToString()</Strong>

            <div class=" dropdown-divider"></div>
            <h3><Strong><span class="fa fa-user"></span> Student Registration Id  : @Html.DisplayFor(model => model.STU_ID)</Strong></h3>

        </div>
    </div>

    <div class="card-body">




