<div class="card" id="pdfpreview" style="padding:35px;margin-top:100px;display:none">


    <div class="row">
        <div class="col-lg-6 col-md-6">
            <h4 class="card-title text-left"><span class="fa fa-file"></span> PDF Generated On @DateTime.UtcNow</h4>
        </div>
        <div class="col-lg-6 col-md-6 text-right">

            <button class="btn btn-sm btn-primary" id="showform"><span class="fa fa-eye "> Show Form</span></button>

        </div>
    </div>

    <div class="row">
        
       <br />
        <div class="col-lg-12 col-md-12 text-center">
            <button class="btn btn-sm btn-success" id="ZoomIn"><span class="fa fa-plus-circle"></span> ZoomIn</button>
            <button class="btn btn-sm btn-danger" id="ZoomOut"><span class="fa fa-plus-circle ">ZoomOut</span></button>

        </div>
    </div>

    <div class="dropdown-divider">   </div>
    <div class="card-body">
        <div class="dropdown-divider">   </div>
        <div class="table-responsive">
            <iframe class="card-frame" id="pdfframe" style="width:-webkit-fill-available;height:500px;" src="~/Registration/GeneratePDF?RegistrationID=@Model.STU_ID"></iframe>
        </div>
    </div>
</div>



<script src="~/Scripts/jquery-1.10.2.min.js"></script>

